{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@action/utils/(.*)": "<rootDir>/../libs/utils/src/$1",
    "@action/utils": "<rootDir>/../libs/utils/src",
    "@action/split-bill/(.*)": "<rootDir>/../libs/split-bill/src/$1",
    "@action/split-bill": "<rootDir>/../libs/split-bill/src",
    "@action/registry/(.*)": "<rootDir>/../libs/registry/src/$1",
    "@action/registry": "<rootDir>/../libs/registry/src",
    "@app/my-test-action/(.*)": "<rootDir>/../libs/my-test-action/src/$1",
    "@app/my-test-action": "<rootDir>/../libs/my-test-action/src"
  }
}